{
  "name": "@ionic/app-scripts-tiny",
  "version": "3.2.1-tiny.3",
  "description": "Scripts for Ionic Projects",
  "publishConfig": {
    "registry": "https://maven.dev.powwowinc.net/repository/npm-powwow"
  },
  "homepage": "https://ionicframework.com/",
  "author": "Ionic Team <hi@ionic.io> (https://ionic.io)",
  "license": "MIT",
  "files": [
    "bin/",
    "config/",
    "dist/",
    "lab",
    "LICENSE",
    "README.md"
  ],
  "bin": {
    "ionic-app-scripts-tiny": "bin/ionic-app-scripts-tiny.js"
  },
  "scripts": {
    "build": "npm run clean && tsc",
    "build-and-test": "npm run build && npm run test",
    "changelog": "./node_modules/.bin/conventional-changelog -p angular -i CHANGELOG.md -s",
    "clean": "rimraf ./dist",
    "github-release": "node ./scripts/create-github-release.js",
    "lint": "tslint -c ./tslint.json --project ./tsconfig.json --type-check -t stylish",
    "nightly": "npm run build && node ./scripts/publish-nightly.js",
    "test": "jest --coverage",
    "watch": "npm run clean && tsc --watch"
  },
  "main": "dist/index.js",
  "dependencies": {
    "@angular-devkit/build-optimizer": "0.0.35",
    "dotenv-webpack": "^1.5.7",
    "fs-extra": "^4.0.2",
    "glob": "^7.1.2",
    "json-loader": "^0.5.7",
    "node-sass": "^4.10.0",
    "os-name": "^2.0.1",
    "source-map": "^0.6.1",
    "tslint": "^5.8.0",
    "tslint-eslint-rules": "^4.1.1",
    "webpack": "3.12.0"
  },
  "devDependencies": {
    "@angular/animations": "5.0.3",
    "@angular/common": "5.0.3",
    "@angular/compiler": "5.0.3",
    "@angular/compiler-cli": "5.0.3",
    "@angular/core": "5.0.3",
    "@angular/forms": "5.0.3",
    "@angular/http": "5.0.3",
    "@angular/platform-browser": "5.0.3",
    "@angular/platform-browser-dynamic": "5.0.3",
    "@angular/platform-server": "5.0.3",
    "@types/fs-extra": "^4.0.8",
    "@types/glob": "^5.0.35",
    "@types/jest": "^21.1.5",
    "@types/mock-fs": "^3.6.30",
    "@types/node": "^8.10.9",
    "@types/node-sass": "^3.10.32",
    "@types/rewire": "^2.5.27",
    "@types/webpack": "^3.8.11",
    "@types/ws": "^3.2.0",
    "conventional-changelog-cli": "^1.3.22",
    "github": "0.2.4",
    "ionic-cz-conventional-changelog": "^1.0.0",
    "jest": "^21.2.1",
    "mock-fs": "^4.4.2",
    "rewire": "^2.5.2",
    "rimraf": "^2.6.1",
    "rxjs": "^5.5.10",
    "sw-toolbox": "^3.6.0",
    "tslint-ionic-rules": "0.0.8",
    "typescript": "~2.4.2",
    "zone.js": "^0.8.26"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/powwowinc/ionic-app-scripts-tiny"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/ionic-cz-conventional-changelog"
    }
  },
  "typings": "dist/index.d.ts",
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts)$": "<rootDir>/preprocessor.js"
    },
    "testRegex": "/src/.*\\.spec\\.(ts|js)$",
    "coverageDirectory": "coverage"
  }
}
